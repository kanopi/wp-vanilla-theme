{
  "name": "kanopi-wordpress-starter-theme",
  "version": "1.1.0",
  "repository": {
    "type": "wordpress-theme-custom",
    "url": "https://github.com/kanopi/wp-vanilla-theme"
  },
  "private": true,
  "scripts": {
    "production": "kanopi-pack production",
    "development": "kanopi-pack development"
  },
  "kanopiPackConfig": {
    "devServer": {
      "port": 4200
    },
    "filePatterns": {
      "entryPoints": {
        "customizer": "./assets/src/js/customizer.js",
        "legacy": "./assets/src/js/legacy.js",
        "theme": "./assets/src/scss/theme.scss"
      }
    },
    "styles": {
      "scssIncludes": [
        "./assets/src/scss/shared/utilities.scss"
      ]
    }
  },
  "devDependencies": {
    "breakpoint-sass": "^2.7.1",
    "kanopi-pack": "file:../../../../../kanopi/kanopi-pack",
    "psi": "^4.1.0",
    "sass-toolkit": "^2.10.2",
    "webpagetest": "^0.5.0"
  },
  "engines": {
    "node": ">=14 <16",
    "npm": ">6.14"
  }
}
